package twilight_magical.minecraft_forge_modding.super_world_gem_.items;

import net.minecraft.item.Item;
import net.minecraft.creativetab.CreativeTabs;

public class LoadAluminiumIngotItem extends Item {
	
	public LoadAluminiumIngotItem(String unlocalizedName) {
		super();
		this.setTextureName("super_world_gem:(Al) Aluminium Ingot - [SuperWorldGemMod]");
		this.setUnlocalizedName(unlocalizedName);
		this.setMaxStackSize(64);
		this.setCreativeTab(CreativeTabs.tabMaterials);
	}

}
