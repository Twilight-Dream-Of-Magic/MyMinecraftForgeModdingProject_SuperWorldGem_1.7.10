package twilight_magical.minecraft_forge_mod.super_world_gem.items;

import net.minecraft.item.Item;
import net.minecraft.creativetab.CreativeTabs;

public class LoadExtremelyUnstableToxicLiquid extends Item {
	
	public  LoadExtremelyUnstableToxicLiquid(String unlocalizedName) {
		super();
		this.setTextureName("super_world_gem:ExtremelyUnstableToxicLiquid");
		this.setUnlocalizedName(unlocalizedName);
		this.setMaxStackSize(16);
		this.setCreativeTab(CreativeTabs.tabMaterials);
	}

}
